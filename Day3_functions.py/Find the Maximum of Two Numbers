def find_max(a,b):
    if a > b:
        print("The maximum number is:", a) 
    elif b > a:
        print("The maximum number is:", b)
    else:   
        print("Both numbers are equal:", a)

num1 = int(input("Enter the first number: "))
num2 = int(input("Enter the second number: "))
find_max(num1, num2)
        